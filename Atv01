// 1   Faça uma função para receber um número e mostrar o resultado conforme a tabela abaixo:

function dezenas(valor) {
    let number = valor
    return number
    }
let result = 35
    if (dezenas(result) < 10){
        console.log("o número é unidade")
    
    } else if (dezenas(result) >=10 && dezenas(result) < 100){
        console.log("o número é dezena")
     
    } else if (dezenas(result) >=100 && dezenas(result) < 1000){
        console.log("o número é centena")
      
    } else {
        console.log("plau")
    }
    
// 3      Faça uma função que recebe a idade do usuário e verifique se ele tem 18 anos ou mais. Se a resposta for positiva escrever “maior de idade”, senão “menor de idade”.

function adulto(valor) {
    let idade = valor
    return idade
    }
let resulty = 35
    if (adulto(resulty) > 18){
        console.log("Maior de idade")
    
    } else {
        console.log("Menor de idade")
    }